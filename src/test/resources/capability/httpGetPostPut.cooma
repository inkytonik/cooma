fun (httpClient : HttpGet & HttpPost & HttpPut) {
    def getBody(response : <Left : String, Right : HttpReturn>) String = {
        response match {
            case Left(_) => ""
            case Right(r) => r.body
        }
    }
    {
        x0 = getBody(httpClient.get("")),
        x1 = getBody(httpClient.post("")),
        x2 = getBody(httpClient.put(""))
    }
}
